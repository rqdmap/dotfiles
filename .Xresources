!! URL Operations
URxvt.url-launcher: google-chrome-stable
URxvt.matcher.button: 1
URxvt.perl-ext-common: default,matcher,resize-font,-confirm-paste

!! Scollbar
URxvt.scrollBar: False
URxvt.scrollBar_floating: True
!! Scoll
URxvt.mouseWheelScrollPage: False
URxvt.scrollstyle: plain
URxvt.scrollTtyKeypress: True
URxvt.scrollTtyOutput: False
URxvt.scrollWithBuffer: True
URxvt.saveLines: 3000

!! Cursor
URxvt.cursorBlink: True
!! Border
URxvt.borderLess: False
URxvt.internalBorder: 20


!! Fonts
Xft.dpi: 96
URxvt.letterSpace: 0
URxvt.font: xft:Iosevka Term:style=Regular:antialias=True:size=13,xft:WenQuanYi Micro Hei:style=Regular:size=13,xft: Font Awesome 5 Brands:size=13, xft:Font Awesome 5 Free Solid:size=13
! URxvt.boldFont: xft:Iosevka Nerd Font Mono:style=Bold:antialias=True:size=13,xft:WenQuanYi Micro Hei:style=Bold:size=13

URxvt.iso14755: False
! URxvt.iso14755_52: False
URxvt.geometry: 120x50

!! Shortcuts
URxvt.keysym.Shift-Control-V:	eval:paste_clipboard
URxvt.keysym.Shift-Control-C:	eval:selection_to_clipboard
URxvt.keysym.Control-0:		resize-font:reset
URxvt.keysym.C-minus:		resize-font:smaller
URxvt.keysym.C-equal:		resize-font:bigger

URxvt.keysym.Control-1:		command:\033]720;3\007
URxvt.keysym.Control-2:		command:\033]721;3\007


Xcursor.theme: miku-cursor

!! $HOME/.Xresources

!! General
Xft.dpi: 96
Xcursor.theme: miku-cursor

!! URxvt
!! Input Method
URxvt.preeditType: Root
URxvt.inputMethod: fcitx

!! Display
URxvt.depth: 32
URxvt*inheritPixmap: true
URxvt.transparent: False
! URxvt*shading: 0 to 99 darkens, 101 to 200 lightens
URxvt*shading: 30

!! Colorscheme

! █▓▒░ scorcerer colors
URxvt.foreground:   #ebdbb2
URxvt.background:   #282828
URxvt.cursorColor:  #fbf1c7
! black
URxvt.color0:       #282828
URxvt.color8:       #928374
! red
URxvt.color1:       #cc241d
URxvt.color9:       #fb4934
! green
URxvt.color2:       #98971a
URxvt.color10:      #b8bb26
! yellow
URxvt.color3:       #d79921
URxvt.color11:		#fabd2f
! blue
URxvt.color4:       #458588
URxvt.color12:      #83a598
! magenta
URxvt.color5:       #b16286
URxvt.color13:      #d3869b
! cyan
URxvt.color6:       #689d6a
URxvt.color14:      #8ec07c
! white
URxvt.color7:       #a89984
URxvt.color15:      #ebdbb2


!! URL Operations
URxvt.url-launcher: google-chrome-stable
URxvt.matcher.button: 1
URxvt.perl-ext-common: default,matcher,resize-font,-confirm-paste

!! Scollbar
URxvt.scrollBar: False
URxvt.scrollBar_floating: True
!! Scoll
URxvt.mouseWheelScrollPage: False
URxvt.scrollstyle: plain
URxvt.scrollTtyKeypress: True
URxvt.scrollTtyOutput: False
URxvt.scrollWithBuffer: True
URxvt.saveLines: 3000

!! Cursor
URxvt.cursorBlink: True
!! Border
URxvt.borderLess: False
URxvt.internalBorder: 20


!! Fonts
URxvt.letterSpace: 0
URxvt.font: xft:Iosevka Term:style=Regular:antialias=True:size=13,xft:WenQuanYi Micro Hei:style=Regular:size=13,xft: Font Awesome 5 Brands:size=13, xft:Font Awesome 5 Free Solid:size=13
! URxvt.boldFont: xft:Iosevka Nerd Font Mono:style=Bold:antialias=True:size=13,xft:WenQuanYi Micro Hei:style=Bold:size=13

URxvt.iso14755: False
! URxvt.iso14755_52: False
URxvt.geometry: 120x50

!! Shortcuts
URxvt.keysym.Shift-Control-V:	eval:paste_clipboard
URxvt.keysym.Shift-Control-C:	eval:selection_to_clipboard
URxvt.keysym.Control-0:		resize-font:reset
URxvt.keysym.C-minus:		resize-font:smaller
URxvt.keysym.C-equal:		resize-font:bigger

URxvt.keysym.Control-1:		command:\033]720;3\007
URxvt.keysym.Control-2:		command:\033]721;3\007

